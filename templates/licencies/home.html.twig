<h2>Liste des Licencies</h2>
<a href="{{ path('liste_licencies_all') }}"> Créer </a>

<!-- LINEARICONS -->
<link rel="stylesheet" href="{{ asset('../public/fonts/linearicons/style.css') }}">

<!-- STYLE CSS -->
<link rel="stylesheet" href="{{ asset('../public/css/style.css') }}">

<ul>
    {% if licencie is defined and (licencie is iterable) %}
        {% for lic in licencie %}
            <div class="container">
                <li>
                    <strong>Numero du Licencie :</strong> {{ lic.getNum() }},
                    <strong>Nom :</strong> {{ lic.getNom() }}
                    <strong>Prenom:</strong> {{ lic.getPrenom() }},
                    <strong>Code la categorie:</strong> {{ lic.getCodeRaccourci() }},

                    <a href="{{ path('update', {'id': lic.getNum()}) }}"> Modifier</a>

                    <a href="{{ path('delete', {'id': lic.getNum()}) }}"> Supprimer </a>

                    <!-- Bouton d'importation -->
                    <input type="file" id="importFile" style="display: none;">
                    <button onclick="$('#importFile')[0].click();">Importer Licenciés</button>

                    <!-- Bouton d'exportation -->
                    <button id="exportButton">Exporter Licenciés</button>

                    <!-- Code d'Exportation et' d'importation -->
                    <script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
                </li>
            </div>
            <br>
        {% endfor %}
    {% endif %}
</ul>
