{% extends 'base.html.twig' %}
{% block body %}
    <div class="container mt-4">
        <h1 class="text-center">Licenciés de la catégorie {{ categories.getNomCat() }}</h1>
        <table class="table table-bordered table-hover mx-auto">
            <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Nom</th>
                <th scope="col">Prénom</th>
            </tr>
            </thead>
            <tbody>
            {% for licencie in licencies %}
                <tr>
                    <th scope="row">{{ loop.index }}</th>
                    <td>{{ licencie.getNomLicencie() }}</td>
                    <td>{{ licencie.getPrenomLicencie() }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}